{
    "manifest_version": 2,
    "name": "Reading Time",
    "description": "Add the reading time to Chrome Extension documentation articles",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "init.js"
            ],
            "run_at": "document_start",
            "all_frames": true
        }
    ]
}